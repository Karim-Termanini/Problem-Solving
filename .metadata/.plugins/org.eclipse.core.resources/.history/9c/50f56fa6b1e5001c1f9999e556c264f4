import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n1 = in.nextInt();
		int n2 = in.nextInt();
		String sum1;
		String sum2;
		sum1 =Integer.toBinaryString(n1);
		sum2 =Integer.toBinaryString(n1);
		System.out.println(sum1);

	}

}
