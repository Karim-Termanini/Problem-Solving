import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n1 = in.nextInt();
		int n2 = in.nextInt();
		int n3 = in.nextInt();
		int max = Math.max(n1, Math.max(n2, n3));
		int min = Math.min(n1, Math.min(n2, n3));
		System.out.print(min + " " + max);
	}

}
