import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		int n1 = num / 10;
		int n2 = num % 10;
		System.out.println(n1 % n2 );
		if (num <= 10 || num >= 100) {
			System.out.println("NO");
		} else {
			if (n1 % n2 == 0) {
				System.out.println("yes");
			}else {
				System.out.println("NO");
			}
		}

	}

}
