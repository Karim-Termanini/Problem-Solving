import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n1, n2, n3, n4;
		n1 = in.nextInt();
		n2 = in.nextInt();
		n3 = in.nextInt();
		n4 = in.nextInt();
		if (n1 == 0 || n2 == 0 || n3 == 0 || n4 == 0) {
			System.out.println("ERror");
		}
	}

}
