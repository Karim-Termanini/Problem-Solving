import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int counter = in.nextInt();
		while(counter!=0) {
			String num = in.nextLine();
			for(int i=0;i<num.length();i++) {
				System.out.println(num.charAt(i) + " ");
			}
			counter--;
		}

	}

}
