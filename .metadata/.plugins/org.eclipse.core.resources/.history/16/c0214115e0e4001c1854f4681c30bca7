import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n1, n2, n3, n4;
		n1 = in.nextInt();
		n2 = in.nextInt();
		n3 = in.nextInt();
		n4 = in.nextInt();
		int f1 = 1, f2 = 1;
		for (int i = 1; i <= n2; i++) {
			f1 = f1 * n1;

		}
		for (int i = 1; i <= n4; i++) {
			f2 = f2 * n3;
		}
		if (f1 > f2) {
			System.out.println("YES");
		} else {
			System.out.println("NO");
		}
	}

}
