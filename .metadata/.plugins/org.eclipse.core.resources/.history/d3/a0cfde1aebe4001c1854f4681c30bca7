import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in;
		in = new Scanner(System.in);
		double discount;
		discount = in.nextDouble();
		double price;
		price = in.nextDouble();
		String s1;
		double d1;
		double d2;
		double d3;
		d3 = price * discount;
		d2 = d3 / (100 - discount);
		d1 = price + d2;
		s1 = String.format("%.2f", d1);
		System.out.println(s1);
	}

}
