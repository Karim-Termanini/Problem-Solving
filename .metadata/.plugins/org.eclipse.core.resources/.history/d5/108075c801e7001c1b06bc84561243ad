import java.util.Scanner;

public class Main {

	public static void main(String[] s0) {
		int days;
		Scanner s1;
		s1 = new Scanner(System.in);
		days = s1.nextInt();
		int year;
		year = days / 365;
		int i1 = 365 * year;
		days = days - i1;
		int month = days / 30;
		days -= 30 * month;
		int day = days;
		System.out.println(year);
		System.out.println(month);
		System.out.println(day);
	}

}
