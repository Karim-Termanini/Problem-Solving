import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n1 = in.nextInt(), n2 = in.nextInt();
		int even = 0, odd = 0;
		if (n1 >= n2) {
			System.out.println("NO");
		} else {
			for (int i = n1; i <= n2; i++) {
				if (i % 2 == 0) {
					even++;
				} else {
					odd++;
				}
			}
			if (odd == 2 && even == 3) {
				System.out.println("Yes");
			}
		}
	}

}
