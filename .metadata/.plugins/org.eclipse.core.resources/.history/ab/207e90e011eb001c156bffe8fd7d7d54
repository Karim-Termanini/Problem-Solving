import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int counter = in.nextInt();
		int rem = 0;
		int revers = 0;
		int n = 0;
		while (counter != 0) {
			n = in.nextInt();
			rem = n % 10;
			revers = revers * 10 + rem;
			System.out.println(revers);
			n/=10;
			counter--;
		}
	}

}
