import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		boolean isPrime = true;
		int div = num / 2;
		for (int i = 2; i < div; i++) {
			if (i == 2) {
				System.out.println(i);
			} else if (num % i == 1) {
				System.out.println(i);
			}
		}
	}

}
