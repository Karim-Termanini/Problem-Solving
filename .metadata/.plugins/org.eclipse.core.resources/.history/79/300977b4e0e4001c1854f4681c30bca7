import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n1, n2, n3, n4;
		n1 = in.nextInt();
		n2 = in.nextInt();
		n3 = in.nextInt();
		n4 = in.nextInt();
		int sum1 = (int) Math.pow(n1, n2);
		int sum2 = (int) Math.pow(n3, n4);
		System.out.println(sum1);
		System.out.println(sum2);
	}

}
