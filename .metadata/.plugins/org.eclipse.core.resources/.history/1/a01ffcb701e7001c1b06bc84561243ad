import java.util.Scanner;

public class Main {

	public static void main(String[] s0) {
		Scanner in;
		in = new Scanner(System.in);
		int days = in.nextInt();
		int year = days / 365;
		days -= 365 * year;
		int month = days / 30;
		days -= 30 * month;
		int day = days;
		System.out.println(year);
		System.out.println(month);
		System.out.println(day);
	}

}
