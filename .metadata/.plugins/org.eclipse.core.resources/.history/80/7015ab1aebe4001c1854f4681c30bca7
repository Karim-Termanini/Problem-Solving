import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in;
		in = new Scanner(System.in);
		double discount;
		discount = in.nextDouble();
		double price;
		price = in.nextDouble();
		String s1 = String.format("%.2f", price + price * discount / (100 - discount));
		System.out.println(s1);
	}

}
