import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		char ch;
		String s1;
		Scanner s2;
		s2 = new Scanner(System.in);
		s1 = s2.next();
		ch = s1.charAt(0);
		char c1;
		if (ch == 'z')
			c1 = 'a';
		else
			c1 = (char) (ch + 1);
		System.out.println(c1);
	}

}