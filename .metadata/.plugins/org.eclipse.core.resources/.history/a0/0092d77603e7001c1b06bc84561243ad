import java.util.Scanner;

public class Main {

	public static void main(String[] s0) {
		Scanner in = new Scanner(System.in);
		int discount = in.nextInt(), price = in.nextInt();
		int sale = 100 - discount;
		int dprice = price / sale;
		int dsale = dprice * discount;
		int total = dsale + price;
		System.out.println(total);

	}

}
