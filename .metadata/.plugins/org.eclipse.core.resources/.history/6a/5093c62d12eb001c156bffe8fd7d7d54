import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int counter = in.nextInt();
		int rem = 0;
		int n = 0;
		while (counter != 0) {
			rem = 0;
			n = in.nextInt();
			while (n != 0) {
				rem = rem * 10 + n % 10;
				
				n /= 10;
			}
			System.out.println(rem + " ");
			counter--;
		}
		
	}

}
