import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int x = in.nextInt();
		int year = x - 365;
		x -= 365 * year;
		int month = x / 30;
		x -= 30 * month;
		int day = x;
		System.out.println(year);
		System.out.println(month);
		System.out.println(day);
	}

}
