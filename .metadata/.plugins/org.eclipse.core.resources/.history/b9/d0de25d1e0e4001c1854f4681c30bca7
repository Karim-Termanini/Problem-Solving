import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		long n1, n2, n3, n4;
		n1 = in.nextInt();
		n2 = in.nextInt();
		n3 = in.nextInt();
		n4 = in.nextInt();
		long sum1 = (long) Math.pow(n1, n2);
		long sum2 = (long) Math.pow(n3, n4);
		if (sum1 > sum2) {
			System.out.println("YES");
		} else {
			System.out.println("NO");
		}
	}

}
