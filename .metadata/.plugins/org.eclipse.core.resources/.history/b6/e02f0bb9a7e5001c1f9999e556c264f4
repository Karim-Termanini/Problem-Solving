import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int num = 2234;
		int counter = 0;
		int firstDigit = num;

		while (num != 0) {
			counter++;
			num /= 10;
		}
		System.out.println(firstDigit);
		System.out.println(counter);
	}

}
